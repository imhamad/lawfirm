{%
set classes = [
'block',
'block-' ~ configuration.provider|clean_class,
'block-' ~ plugin_id|clean_class,
]

%}
<section class="section section-default section-no-border mt-none">
	<div{{ attributes.addClass(classes) }}>
    {{ title_prefix }}
		<div class="container">
			<div class="row mb-xl">
				<div class="col-md-7">
					{{content.field_who_we_are}}
					
					<a class="mt-md" href="{{content.field_learn_more | render | striptags | trim }}">Learn More <i class="fa fa-long-arrow-right"></i></a>

				</div>
				<div class="col-md-4 col-md-offset-1">
					{{content.field_our_commitment}}
				</div>
			</div>
		</div>
		{{ title_suffix }}
	</div>
</section>
